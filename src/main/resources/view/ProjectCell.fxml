<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" prefHeight="25.0" prefWidth="240.0" stylesheets="@/Style.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="projectNameLabel" prefHeight="25.0" prefWidth="100.0" styleClass="labDark" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <padding>
            <Insets left="5.0" />
         </padding>
      </Label>
      <ImageView fx:id="editProjectBtn" fitHeight="20.0" fitWidth="20.0"  preserveRatio="true" AnchorPane.rightAnchor="21.0" AnchorPane.topAnchor="2.5">
         <image>
            <Image url="@/icons/icons8-edit-48.png" />
         </image>
      </ImageView>
      <ImageView fx:id="deleteProjectBtn" fitHeight="20.0" fitWidth="20.0" preserveRatio="true" AnchorPane.rightAnchor="0.5" AnchorPane.topAnchor="2.5">
         <image>
            <Image url="@/icons/sharp_delete_forever_black_18dp.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
